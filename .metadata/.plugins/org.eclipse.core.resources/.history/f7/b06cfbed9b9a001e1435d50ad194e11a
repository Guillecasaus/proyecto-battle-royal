package ProyectoFinalJava;

public class HerramientaArco extends Herramientas{
	public static final Double ARCO_BONUS = 1.6;
	public static final Integer USOS = 3;
	private Integer usosRestantes;
	
	public HerramientaArco(String nombre) {
		super(nombre, ARCO_BONUS, TipoArma.arco);
		this.usosRestantes = HerramientaArco.USOS;
	}
}
