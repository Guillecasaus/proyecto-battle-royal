package ProyectoFinalJava;

public class HerramientaBaston extends Herramientas{
	public static final Double BASTON_BONUS = 1.2;
	public static final Integer USOS = 3;
	private Integer usosRestantes;
	
	public HerramientaBaston(String nombre) {
		super(nombre, BASTON_BONUS, TipoArma.baston);
		this.usosRestantes = HerramientaBaston.USOS;
	}
}
